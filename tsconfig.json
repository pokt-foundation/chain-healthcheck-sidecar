{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "paths": {
            "@src/*": [
                "./src/*"
            ],
            "@chains/*": [
                "./src/chains/*"
            ],
            "@chains": [
                "./src/chains/index"
            ],
        }
    },
    "lib": [
        "es2021"
    ]
}